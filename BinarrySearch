#include <bits/stdc++.h>
using namespace std;
int binarrySearch(vector<int>&v,int n,int x){
    int l=0,r=n-1;
    while(l<r){
        int mid=(l+r)/2;
        if(v[mid]==x)
        return mid;
        else if(v[mid]<x)
        l++;
        else
        r--;
    }
    return -1;
}

int main() {
	// your code goes here
	int n,x;
	cin>>n>>x;
	vector<int>v(n);
	for(int i=0;i<n;i++)
	cin>>v[i];
	sort(v.begin(),v.end());
	 int z=binarrySearch(v,n,x);
	 if(z==-1)
	 cout<<"Not found";
	 else
	 cout<<"found at index"<<" "<<z;
	
 
	
}
