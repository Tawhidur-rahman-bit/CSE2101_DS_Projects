#include <bits/stdc++.h>
using namespace std;
void deleteAtIndex(vector<int>&v,int idx){
    int n=v.size();
    for(int i=idx;i<n-1;i++){
        v[i]=v[i+1];
    }
    v.resize(n-1);
}


int main() {
	// your code goes here
	int n,x;
	cin>>n>>x;
	vector<int>v(n);
	for(int i=0;i<n;i++)
	cin>>v[i];
	deleteAtIndex(v,x);
	for(int y:v)
	cout<<y<<" ";

}
